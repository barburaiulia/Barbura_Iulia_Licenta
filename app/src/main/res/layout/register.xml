<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#20b2aa">

    <TextView

        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="First Name"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idNumeTxt"
        app:layout_constraintStart_toStartOf="@+id/idNumeTxt" />

    <EditText
        android:id="@+id/idNumeTxt"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/rectangle"

        android:paddingLeft="10dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/idUserNameTxt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182" />

    <TextView

        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Last Name"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idPrenumeTxt"
        app:layout_constraintStart_toStartOf="@+id/idPrenumeTxt" />

    <EditText
        android:id="@+id/idPrenumeTxt"
        android:layout_width="120dp"
        android:layout_height="40dp"

        android:background="@drawable/rectangle"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/idUserNameTxt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/idNumeTxt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182" />

    <TextView

        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Username"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idUserNameTxt"
        app:layout_constraintStart_toStartOf="@+id/idUserNameTxt" />

    <EditText
        android:id="@+id/idUserNameTxt"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:background="@drawable/rectangle"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idNumeTxt"
        app:layout_constraintVertical_bias="0.154" />

    <TextView

        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Password"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idPasswordTxt"
        app:layout_constraintStart_toStartOf="@+id/idPasswordTxt" />

    <EditText
        android:id="@+id/idPasswordTxt"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:background="@drawable/rectangle"
        android:paddingLeft="10dp"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.69" />

    <TextView

        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Confirm password"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idPasswordTxtVer"
        app:layout_constraintStart_toStartOf="@+id/idPasswordTxtVer" />

    <EditText
        android:id="@+id/idPasswordTxtVer"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:background="@drawable/rectangle"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.853" />

    <Button
        android:id="@+id/idRegisterButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_2"
        android:text="Register"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idPasswordTxtVer"
        app:layout_constraintVertical_bias="0.777" />

    <TextView

        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Facultate"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idFacultateText"
        app:layout_constraintStart_toStartOf="@+id/idFacultateText" />

    <EditText
        android:id="@+id/idFacultateText"
        android:layout_width="82dp"
        android:layout_height="38dp"
        android:layout_marginStart="156dp"
        android:background="@drawable/rectangle"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505" />

    <TextView

        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="An"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idAnText"
        app:layout_constraintStart_toStartOf="@+id/idAnText" />

    <EditText
        android:id="@+id/idAnText"
        android:layout_width="53dp"
        android:layout_height="38dp"
        android:background="@drawable/rectangle"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.506" />

    <TextView

        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        android:text="Specializare"
        android:textAppearance="@style/the_place_t"
        app:layout_constraintBottom_toTopOf="@+id/idSpecializareText"
        app:layout_constraintStart_toStartOf="@+id/idSpecializareText" />

    <EditText
        android:id="@+id/idSpecializareText"
        android:layout_width="80dp"
        android:layout_height="38dp"
        android:background="@drawable/rectangle"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.504" />


</androidx.constraintlayout.widget.ConstraintLayout>